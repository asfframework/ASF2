package  {		import flash.display.MovieClip;	import asf.view.BaseLoaderApplication;	import asf.plugins.loadermax.LoaderMaxFactoryPlugin;	import flash.net.URLRequest;	import asf.core.viewcontrollers.ButtonViewController;	import asf.events.DependenciesProgressEvent;	import flash.events.MouseEvent;		public class Sounds1 extends BaseLoaderApplication 	{		{			Cache;		}				private var btHome:ButtonViewController;		private var btImages:ButtonViewController;				public function Sounds1( ) 		{			var states:Object;						super( new LoaderMaxFactoryPlugin( ) );						$menu1.stop( );			$menu2.stop( );						$menu1.visible = false;			$menu2.visible = false;						loadApplicationConfigFile( new URLRequest( "sounds_1.xml" ) );						addChild( app.view );		}				protected override function appLoadComplete(event:DependenciesProgressEvent):void		{			var states:Object;						states = { 				over: app.sounds.getSoundItem( "over" ),				out: app.sounds.getSoundItem( "out" ),				click: app.sounds.getSoundItem( "click" ) 			};						btHome = new ButtonViewController( $menu1, states );			btImages = new ButtonViewController( $menu2, states );						btHome.view.$label.text = "Home";			btImages.view.$label.text = "Soundtrack";						btHome.view.visible = true;			btImages.view.visible = true;						btHome.addEventListener( MouseEvent.CLICK, onMenuClick );			btImages.addEventListener( MouseEvent.CLICK, onMenuClick );		}				private function onMenuClick( evt:MouseEvent ):void		{			switch( evt.target )			{				case btHome:				{					app.navigation.openSection( "home" );					break;				}				case btImages:				{					app.navigation.openSection( "soundtrack" );					break;				}			}		}	}}