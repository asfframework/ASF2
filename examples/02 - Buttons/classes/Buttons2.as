package  {		import flash.display.MovieClip;	import asf.core.viewcontrollers.ButtonViewController;	import asf.core.app.ASF;	import asf.plugins.loadermax.LoaderMaxFactoryPlugin;	import asf.events.DependenciesProgressEvent;	import flash.net.URLRequest;			public class Buttons2 extends MovieClip {				public var $bt1:MovieClip;		public var $bt2:MovieClip;				private var btVC1:ButtonViewController;		private var btVC2:ButtonViewController;				private var app:ASF;				public function Buttons2( ) 		{			$bt1.stop( );			$bt2.stop( );						app = new ASF( this, root.loaderInfo, new LoaderMaxFactoryPlugin( ) );						app.addEventListener( DependenciesProgressEvent.LOAD_COMPLETE, onAppLoad );						app.loadModel( new URLRequest( "buttons_02.xml" ) );		}				private function onAppLoad( evt:DependenciesProgressEvent ):void		{			var states:Object = {				over: app.sounds.getSoundItem( "over" ),				out: app.sounds.getSoundItem( "out" ),				click: app.sounds.getSoundItem( "click" )			};					btVC1 = new ButtonViewController( $bt1, states );			btVC2 = new ButtonViewController( $bt2, states );		}	}	}